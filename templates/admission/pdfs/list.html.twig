<table style="width: 100% ; background: #FFFFFF;font-size: 12px" cellspacing="0" class="slh">
    <thead>
        <tr style="background-color: rgb(31, 79, 122);font-size: 10px;">
            <th style="width: 7%;text-align: left; padding:6px;border-right:1px solid white;font-weight: bold;color: white;font-size: 10px">ORD</th>
            <th style="width: 23%;text-align: left; padding:6px;border-right:1px solid white;font-weight: bold;color: white;font-size: 10px">Code Admission</th>
            <th style="width: 20%;text-align: left; padding:6px;border-right:1px solid white;font-weight: bold;color: white;font-size: 10px">Nom</th>
            <th style="width: 20%;text-align: left; padding:6px;border-right:1px solid white;font-weight: bold;color: white;font-size: 10px">Prénom</th>
            <th style="width: 15%;text-align: left; padding:6px;border-right:1px solid white;font-weight: bold;color: white;font-size: 10px">CIN</th>
            <th style="width: 15%;text-align: left; padding:6px;border-right:1px solid white;font-weight: bold;color: white;font-size: 10px">CNE</th>
        </tr>
    </thead>
    <tbody>
        {% for admission in admissions %}
        <tr>
            <td style="padding:6px;border-right:1px solid white;text-align: left;">{{loop.index}}</td>
            <td style="padding:6px;border-right:1px solid white;text-align: left;">{{admission.code}}</td>
            <td style="padding:6px;border-right:1px solid white;text-align: left;">{{admission.preinscription.etudiant.nom}}</td>
            <td style="padding:6px;border-right:1px solid white;text-align: left;">{{admission.preinscription.etudiant.prenom}}</td>
            <td style="padding:6px;border-right:1px solid white;text-align: left;">{{admission.preinscription.etudiant.cin}}</td>
            <td style="padding:6px;border-right:1px solid white;text-align: left;">{{admission.preinscription.etudiant.cne}}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>